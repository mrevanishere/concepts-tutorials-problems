// 70. Climbing Stairs
// DP:
// sum of previous two
// modified fib
class Solution {
public:
    int climbStairs(int n) {
        int f[n+2];
        int i;
        f[1] = 1;
        f[2] = 2;
        for (i = 3; i <= n; ++i) {
            f[i] = f[i-1] + f[i-2];
        }
        return f[n];
    }
};
